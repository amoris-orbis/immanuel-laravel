{
    "name": "theriftlab/immanuel-laravel",
    "description": "Laravel facade for easy integration with the Immanuel astrology API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Davies",
            "email": "robert@theriftlab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RiftLab\\Immanuel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiftLab\\Immanuel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiftLab\\Immanuel\\ImmanuelServiceProvider"
            ],
            "aliases": {
                "Immanuel": "RiftLab\\Immanuel\\Facades\\Immanuel"
            }
        }
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "phpunit/phpunit": "^9.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
